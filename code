<script src="jquery.slidereveal.min.js"></script>
<script type="text/javascript" language="javascript">
function openPanelNow(showhide, qreload, qtype, qval, qwidth)
{
 	if(qwidth==0 || qwidth=="") qwidth="90%";
	if(qreload==0 || qreload=="") qreload="0";
 	
	showProgress();
	if(showhide==0) showhide="hide";
	else if(showhide==1) showhide="show";
	$("#slider007").show();
     var slider = $("#slider007").slideReveal({
          width:""+qwidth,
          push: false,
          position: "right",
          speed: 200,
          trigger: $("#trigger"),
          overlay: true,
		  show: function(){
			  openMyPanelContentsBAk(qtype, qval);
  			},
		hidden: function()
				{
 					if($.trim(qreload)==1) location.reload(); 
				} 
		
        });
		
	slider.slideReveal(""+showhide);	
}


function openMyPanelContentsBAk(qtype, qval)
{

	 $.ajax({
										type: 'POST',
 										data: "qtype="+qtype+"&qval="+qval,
										url: 'eims_files/openmypanelscontentsBAK.php',
										success: function(htmldata) 
										{
 														$('#PanelcontentArea').html(htmldata);
														hideProgress(); 
														
														
 										} 
								});  
								
								
}
</script>
     



