<script type="application/javascript" language="javascript">
	function showProgress()
	{
		$('#loadmeplz').ajaxStart(function() { $(this).show(); });
	}
	
	function hideProgress()
	{
		$('#loadmeplz').ajaxComplete(function() { $(this).hide(); });
	}
</script>

        
