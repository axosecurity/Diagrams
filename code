<script type="text/javascript" language="javascript">
	function CreateMyAdmitCard_PDF(exam_id, student_id)
	{
		
 		var examURL=exam_id+"_"+student_id+"_admitCard.pdf";
 		showProgress();
		$.ajax({
										type: 'GET',
										url: 'createAdmitCardPDF_online.php',
										data: "eid="+exam_id+"&sid="+student_id,
										success: function(htmldata) 
										{
											 swal("Successful", "PDF Created Successfully\nSave or Check Download Folder","success");
											 hideProgress();
 										},
										
										complete: function()
										{
											window.location="https://ssaac.edu.bd/result_pdfs/"+examURL;
										}
								});
		
 		
		
	}
</script>










